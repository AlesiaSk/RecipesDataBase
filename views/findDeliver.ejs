<% include partials/header.ejs %>
    <div class="container">
        <% if (message != '') { %>
            <p class="text-center text-danger"><%= message %></p>
        <% } %>
        <form class="add-price-form" action="" method="post" enctype="multipart/form-data">
            <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="prname">Name of provider</label>
                        <input type="text" class="form-control" name="prname" id="prname" placeholder="Name" required>
                    </div>
                    <div class="form-group col-md-6">
                        <label for="dateDel">Date</label>
                        <input type="text" class="form-control" name="dateDel" id="dateDel" placeholder="Date" required>
                    </div>
                </div>
            <button type="submit" class="btn btn-primary float-right">Search</button>
        </form>
    </div>
</div>
</body>
</html>